{
    "version": 1,
    "rules": {
        "default": {
            "groupBy": "namespace",
            "target": [
                "post id",
                "title"
            ],
            "fallback": "uncategorized",
            "sort_in_group": [
                "namespace:page:asc"
            ]
        },
        "overrides": [
            {
                "tag_match": "creator:nope",
                "groupBy": "time",
                "interval": "month"
            }
        ]
    }

}

